<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.foodheaven.foodheaven.foodheaven">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Cuisines"/>
        <activity android:name=".cusine_details"
            android:windowSoftInputMode="adjustPan"
            />
        <activity android:name=".order_details"
            android:windowSoftInputMode="adjustPan"
            />
        <activity android:name=".finalizing"/>
        <activity android:name=".orderview"/>





    </application>

</manifest>
